                    {
                        "name": "password_confirmation",
                        "in": "query",
                        "description": "Confirm Password",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "device_token",
                        "in": "query",
                        "description": "Device Token",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "User Registered Successfully"
